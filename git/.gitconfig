[alias]
    d = diff
    co = checkout
    br = branch
    st = status
    ci = commit
    cia = commit --all
    cim = commit --amend
    cima = commit --amend --all
    dfl = diff --name-status
    dfs = diff --staged
    f = fetch --tags
    last = log -1 HEAD
    logg = log --graph --decorate --oneline --abbrev-commit --all
    logo = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    logs = log --all --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    p = push
    puo = push o
    rb = rebase
    rbc = rebase --continue
    rbi = rebase -i
    rs = reset HEAD^
    rh = reset --hard
    rroo = remote rename origin o
    slog = log --graph --decorate --oneline --all --simplify-by-decoration
    squash = rebase HEAD~2 -i
    slist = stash list
    spop = stash pop
    sdrop = stash drop
    sshow = stash show -p stash@{0}

    back = reset --soft HEAD^
    cancel = restore --staged .
[core]
    editor = nvim
    autocrlf = false
    ignorecase = false
    quotepath = false
    excludesfile = ~/.gitignore
[color]
    ui = true
[merge]
    tool = vimdiff
